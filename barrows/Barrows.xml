<manifest>
    <main-class>com.stixx.bots.barrows.Barrows</main-class>
    <name>Barrows Bot</name>
    <tag-line>Kill the bros</tag-line><!--Max of 50 chars-->
    <description>No reqs, easy money</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>MONEYMAKING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>PowerChopper</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Tree</tag>
        <tag>Vine</tag>
    </tags>
    <resources>
        <resource>path/to/file.png</resource>
    </resources>
</manifest>